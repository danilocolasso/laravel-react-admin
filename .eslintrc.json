{
    "env": {
        "browser": true,
        "es2020": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "standard",
        "standard-react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/prop-types": "off"
    }
}
